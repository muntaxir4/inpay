{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "start": {
      "persistent": true
    },
    "db:init": {
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "dependsOn": ["^db:init"]
    },
    "db:migrate": {
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "dependsOn": ["^db:migrate"]
    },
    "db:generate": {
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "dependsOn": ["^db:generate"]
    },
    "test:unit": {
      "inputs": ["$TURBO_DEFAULT$"],
      "dependsOn": ["^test:unit"]
    },
    "test:integration": {
      "inputs": ["$TURBO_DEFAULT$", ".env.example"],
      "dependsOn": ["^test:integration"]
    },
    "test:init": {
      "inputs": ["$TURBO_DEFAULT$", ".env.example"],
      "dependsOn": ["^test:init"]
    }
  }
}
